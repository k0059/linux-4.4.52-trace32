          POS 17. 6. 7. 1.
          BUTTON "Cancel"  "DIALOG.END"
        )
      )
      SEPARATOR
      MENUITEM "[:mmu]Display Process MMU Tables"
      (
        DIALOG
        (
            HEADER "MMU.List TaskPageTable"
            POS 0. 0. 24. 3.
            BOX "Process name"
            POS 1. 1. 22. 1.
wproca2:    DEFEDIT "" ""
            POS 2. 3. 7. 1.
            DEFBUTTON "Ok"
            (
              LOCAL &wproca
              &wproca=DIALOG.STRING(wproca2)
              MMU.List TaskPageTable "&wproca"
              DIALOG.END
