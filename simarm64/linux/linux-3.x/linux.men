      (
        DIALOG
        (
          HEADER "TASK.sYmbol.DELeteMod"
          POS 0. 0. 30. 3.
          BOX "Module name"
          POS 1. 1. 20. 1.
modd:     DEFEDIT "" ""
          POS 22. 1. 7. 1.
          BUTTON "Browse..."
          (
            DIALOG.FILE "*.ko"
            ENTRY %LINE &file
            DIALOG.SET modd "&file"
          )
          POS 6. 3. 7. 1.
          DEFBUTTON "Ok"
          (
